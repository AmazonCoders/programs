import java.io.*;
import java.util.*;
class node 
{
    int data;
    node next;
}
class que
{  
    public static void main(String args[])
    {
        Scanner s=new Scanner(System.in);
        node head=null;
        s1 s2=new s1();
        int n;
        do{
        System.out.println("\n1.Enque\n2.Deque\n3.display\n\n");
        n=s.nextInt();
        if(n==1)
        {   System.out.println("Enter a number to enque");
            int i=s.nextInt();
          head= s2.enque(head,i);
        }
        else if(n==2)
            head=s2.deque(head);
        else
        {
            s2.disp(head);
        }
          }while(n<=3);
    }
}
class s1
{
    public node  enque(node head,int i)
    {
        node p=new node();
        p.data=i;
        p.next=null;
        if(head==null)
            head=p;
        else 
        {p.next=head;head=p;}
        return head;
    }
    public node deque(node head)
    {
        if(head==null)
        {System.out.println("The Que is Empty");}
        else
        {
        node temp=head;
        if(temp.next==null){System.out.println("The dequed element is "+temp.data);return head;}
        node temp1=temp.next;
        while(temp1.next!=null)
        {temp=temp.next;temp1=temp1.next;}
       System.out.println("The dequed element is "+temp1.data);
       temp.next=null;
        }
       return head;
     }
    public void disp(node head)
    {
      node temp=head;
        while(temp.next!=null)
        {
            System.out.print(temp.data+"\t");
           temp=temp.next;
        }
             System.out.println(temp.data);
     }
}
